package  {		import flash.display.MovieClip;	import flash.events.Event;		public class Model {				var grid:Grid;				public function Model() {			this.grid = new Grid();		}				public function walkAll():void{			//array mit effektiv ausgewählten einheiten?						for each (var e:Element in grid.elementArray){				e.walk();			}					}				public function getGrid():Grid{			return grid;		}	}	}