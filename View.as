package  {	import flash.display.MovieClip;	import fl.motion.Color;	public class View {		public function View() {				}				public function tintElement(toBeTinted:MovieClip, tintColor:uint, tintMultiplier : Number){			//0 <= tintMultiplier <= 1			var c:Color = new Color();			c.setTint(tintColor,tintMultiplier);			toBeTinted.transform.colorTransform=c;					}				public function tintArray(tintElements:Array, tintColor:uint, tintMultiplier : Number){			for each (var e : MovieClip in tintElements){				tintElement(e,tintColor,tintMultiplier);			}		}	}	}