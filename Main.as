package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;		public class Main extends MovieClip {		var r:Rock;		var s:Scissor;		var p:Paper		var grid:Grid;		var controller:Controller;		var model:Model;		public function Main():void {			addEventListener(Event.ENTER_FRAME, enterFrameHandler);			grid = new Grid();			model = new Model(grid);			controller = new Controller(model);			grid.addEventListener(MouseEvent.MOUSE_UP, controller.gridClickHandler);								  						grid.x += 50;			grid.y += 50;						stage.addChild(grid);						r = new Rock();			s = new Scissor();			p = new Paper();						grid.getTile(10,0).setElement(p);						grid.getTile(0,0).setElement(s);			grid.getTile(0,0).setElement(r);						/*r.gotoTarget(16,5);			s.gotoTarget(0,10);			p.gotoTarget(15,19);*/		}				public function enterFrameHandler(evt:Event):void {			//grid.getTile(0,0).getElement().walk();			model.walkAll();			//trace("main");		}					}	}