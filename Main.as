package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;		public class Main extends MovieClip {		var r:Rock;		var s:Scissor;		var p:Paper		var grid:Grid;		var controller:Controller;		var model:Model;		var view: View;		public function Main():void {			addEventListener(Event.ENTER_FRAME, enterFrameHandler);			model = new Model();			controller = new Controller(model);			view = new View();						var grid:Grid = model.getGrid();			grid.addEventListener(MouseEvent.MOUSE_UP, controller.gridClickHandler);			grid.addEventListener(MouseEvent.MOUSE_OVER, controller.gridHoverHandler);  						grid.x += 50;			grid.y += 50;						stage.addChild(grid);						r = new Rock();			s = new Scissor();			p = new Paper();						var tintColor:uint = uint("0xff4500"); 			view.tintArray(s.tintElements,tintColor,1);						grid.getTile(10,0).createElement(p);			grid.getTile(1,0).createElement(s);			grid.getTile(0,1).createElement(r);								}				public function enterFrameHandler(evt:Event):void {			model.walkAll();		}					}	}