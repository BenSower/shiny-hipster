package  {		import flash.display.MovieClip;	import flash.events.Event;		public class Main extends MovieClip {		var r:Rock;		var s:Scissor;		var p:Paper		public function Main():void {						addEventListener(Event.ENTER_FRAME, enterFrameHandler);						var grid:Grid = new Grid();			var model:Model = new Model(grid);									grid.x += 50;			grid.y += 50;						stage.addChild(grid);						r = new Rock();			s = new Scissor();			p = new Paper();						grid.getTile(10,0).setElement(p);			grid.getTile(24,0).setElement(s);			grid.getTile(0,0).setElement(r);						r.gotoTarget(16,0);			s.gotoTarget(0,0);			p.gotoTarget(24,0);		}				public function enterFrameHandler(evt:Event):void {			r.animate();			s.animate();			p.animate();		}					}	}