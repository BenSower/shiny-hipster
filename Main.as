package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;		public class Main extends MovieClip {		var r:Unit;		var s:Unit;		var p:Unit;		var grid:Grid;		var controller:Controller;		var model:Model;		var view: View;		public function Main():void {			addEventListener(Event.ENTER_FRAME, enterFrameHandler);			view = new View();			model = new Model(view);			controller = new Controller(model);						var grid:Grid = model.getGrid();			grid.addEventListener(MouseEvent.MOUSE_UP, controller.gridClickHandler);			grid.addEventListener(MouseEvent.MOUSE_OVER, controller.gridHoverHandler);  						grid.x += 50;			grid.y += 50;						stage.addChild(grid);						var tintColor1:uint = uint("0xff4500"); 			var tintColor2:uint = uint("0x0046aa"); 			var player1:Player = new Player(tintColor1);			var player2:Player = new Player(tintColor2);			r = model.createUnit("rock", player1);			s = model.createUnit("scissor", player1);			p = model.createUnit("scissor", player2);						trace(player1.units);						grid.getTile(10,0).createElement(p);			grid.getTile(1,0).createElement(s);			grid.getTile(0,1).createElement(r);							}				public function enterFrameHandler(evt:Event):void {			model.walkAll();		}					}	}