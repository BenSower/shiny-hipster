package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;		public class Main extends MovieClip {		var r:Unit;		var s:Unit;		var p:Unit;		var grid:Grid;		var controller:Controller;		var model:Model;		var view: View;		var roundButton: NextRound;		var menue:Menue;				public function Main():void {						//menue = new Menue()			//this.addChild(menue);			addEventListener(Event.ENTER_FRAME, enterFrameHandler);			view = new View();			model = new Model(view);			controller = new Controller(model);			roundButton = new NextRound();			//roundButton.gotoAndStop(1);									var grid:Grid = model.getGrid();			grid.addEventListener(MouseEvent.MOUSE_UP, controller.gridClickHandler);			stage.addEventListener(MouseEvent.MOUSE_UP, controller.stageClickHandler);			grid.addEventListener(MouseEvent.MOUSE_OVER, controller.gridHoverHandler);  						grid.x += 50;			grid.y += 50;			roundButton.x = 1100;			roundButton.y = 500;						stage.addChild(roundButton);			stage.addChild(grid);						model.gameCreation();		}		public function enterFrameHandler(evt:Event):void {			if(model.checkForWalkpath() == false) {				model.walkFlag = false;				var v:int = 0;				model.setActivePlayer(v);			}			if(model.walkFlag){				model.walkAll();			}							}				}	}