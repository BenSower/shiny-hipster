package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;		public class Main extends MovieClip {		var r:Unit;		var s:Unit;		var p:Unit;		var grid:Grid;		var controller:Controller;		var model:Model;		var view: View;		public function Main():void {			addEventListener(Event.ENTER_FRAME, enterFrameHandler);			view = new View();			model = new Model(view);			controller = new Controller(model);									var grid:Grid = model.getGrid();			grid.addEventListener(MouseEvent.MOUSE_UP, controller.gridClickHandler);			grid.addEventListener(MouseEvent.MOUSE_OVER, controller.gridHoverHandler);  						grid.x += 50;			grid.y += 50;						stage.addChild(grid);						model.gameCreation();		}				public function enterFrameHandler(evt:Event):void {			model.walkAll();		}				}	}