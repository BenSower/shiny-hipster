package  {	import flash.display.SimpleButton;	import flash.display.DisplayObject;		public class ScissorInterface extends SimpleButton{				public function ScissorInterface(gx: Number, gy: Number) {			this.x = gx;			this.y = gy;			this.scaleX = this.scaleY = 0.25;		}				public function enableButton():void{			this.enabled = true;		}				public function disableButton():void{			this.enabled = false;		}	}	}