package  {		import flash.display.MovieClip;			public class Tile extends MovieClip {		static const SCALE:Number = 1;		var gx:int;		var gy:int;		var grid:Grid;		var element:Element;		var obstacle:Obstacle;		var elementStack: Array;				public function Tile() {			elementStack = new Array();		}				public function init(gx:int,gy:int):void{			this.gx = gx;			this.gy = gy;/*			x = (gx-gy)*Tile.WIDTH/2;			y = (gx+gy)*Tile.HEIGHT/2;*/			scaleX = SCALE;			scaleY = SCALE;			x = gx*width;			y = gy*height;		}				public function createElement(e:Element):void{			grid.elementArray.push(e);			setElement(e);		}				public function setElement(e:Element):void{			if (elementStack.indexOf(e,0) != -1) return;			e.tile = this;			elementStack.push(e);			addChild(e);		}				public function getElement():Element{			return elementStack[elementStack.length-1];		}				public function getObstacle():Obstacle{			return obstacle;		}				public function popElement():void{			elementStack.pop();		}				public function setObstacle(o:Obstacle):void{			if (obstacle==o) return;			if(obstacle!=null){				removeChild(obstacle);				obstacle = null;			}			o.tile = this;			obstacle = o;			addChild(obstacle);		}	}	}