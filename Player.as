package  {			public class Player {		public static var counter:int = 0;		var id:int;		var color:uint;		//var name:String; Feature		var units:Array;		var base:Base;		//var conqueredTiles:Array;		var light:int = 10;						public function Player(c:uint) {			this.color = c;			id = counter;			counter++;			units = new Array();		}				public function addUnit(unit :Unit){			units.push(unit);		}				public function getLight():int{			return this.light;		}				public function resetCounter():void{			counter--;			counter--;			trace("cpounter: "+counter);		}			}	}